{
  "expo": {
    "name": "DonationWagon",
    "slug": "DonationWagon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/caravan2.jpg",
    "jsEngine": "jsc",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#00000000",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/caravan2.jpg",
        "backgroundColor": "#00000000"
      },
      "package": "com.atzulo.DonationWagon",
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "buildToolsVersion": "34.0.0"

    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/caravan.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "development": {
      "suppressLogs": true,
      "disableErrorOverlay": true,
      "fastRefresh": true,
      "minifyEnabled": false
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
